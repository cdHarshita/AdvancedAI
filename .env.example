# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_ORG_ID=your-org-id-here

# Anthropic (Claude)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Google (Gemini)
GOOGLE_API_KEY=your-google-api-key-here

# Cohere
COHERE_API_KEY=your-cohere-api-key-here

# Vector Databases
PINECONE_API_KEY=your-pinecone-api-key-here
PINECONE_ENV=your-pinecone-environment-here
WEAVIATE_URL=your-weaviate-url-here
WEAVIATE_API_KEY=your-weaviate-api-key-here

# Azure OpenAI
AZURE_OPENAI_API_KEY=your-azure-openai-key-here
AZURE_OPENAI_ENDPOINT=your-azure-endpoint-here
AZURE_OPENAI_DEPLOYMENT=your-deployment-name-here
AZURE_OPENAI_API_VERSION=2024-02-01

# AWS (for Bedrock)
AWS_ACCESS_KEY_ID=your-aws-access-key-here
AWS_SECRET_ACCESS_KEY=your-aws-secret-key-here
AWS_REGION=us-east-1

# HuggingFace
HUGGINGFACE_API_KEY=your-huggingface-token-here

# LangSmith (LangChain monitoring)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your-langsmith-api-key-here
LANGCHAIN_PROJECT=advanced-ai-lab

# Application Settings
LOG_LEVEL=INFO
ENVIRONMENT=development
MAX_TOKENS=4000
TEMPERATURE=0.7

# Database (if needed)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Redis (for caching)
REDIS_URL=redis://localhost:6379

# Monitoring (optional)
SENTRY_DSN=your-sentry-dsn-here

# Cost Limits
MAX_COST_PER_QUERY_USD=1.0
MAX_DAILY_COST_USD=100.0
